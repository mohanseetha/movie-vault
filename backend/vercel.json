{
  "version": 2,
  "builds": [{ "src": "app.py", "use": "@vercel/python" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.py"
    },
    { "src": "/add_review", "dest": "/app.py" },
    { "src": "/delete_review", "dest": "/app.py" },
    { "src": "/get_reviews/(.*)", "dest": "/app.py" },
    { "src": "/get_review", "dest": "/app.py" },
    { "src": "/edit-review", "dest": "/app.py" }
  ]
}
